{
    "name": "toyza55k/laravel-backend-preset",
    "description": "Laravel backend preset and command for laravel-backend",
    "license": "MIT",
    "authors": [
        {
            "name": "Toy Pongsatorn",
            "email": "toyza55k@gmail.com"
        }
    ],
    "homepage": "https://github.com/toyza55k/backend",
    "keywords": ["Laravel", "Backend", "Preset", "Command"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Toyza55k\\Backend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toyza55k\\Backend\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Toyza55k\\Backend\\BackendServiceProvider"
            ],
            "aliases": {
                "Backend": "Toyza55k\\Backend\\Facades\\Backend"
            }
        }
    }
}
